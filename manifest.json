{
  "manifest_version": 3,
  "name": "leetcode helper AI",
  "version": "1.0",
  "description": "Your guide for leetcode",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["src/content.tsx"],
      "matches": ["https://leetcode.com/problems/*"]
    }
  ]
}
